# 개발 환경 설정 예시 파일
# 실제 사용 시 이 파일을 복사하여 application-dev.properties로 저장하고 실제 값으로 교체하세요

# MySQL
spring.datasource.url=jdbc:mysql://localhost:3307/isitempty?useSSL=false&serverTimezone=UTC&allowPublicKeyRetrieval=true
spring.datasource.username=your_db_username
spring.datasource.password=your_db_password

# Redis
spring.data.redis.host=localhost
spring.data.redis.port=6379
spring.data.redis.password=your_redis_password

# 서울시 API 키
seoul.api.key=your_seoul_api_key

# SSH 터널링 설정
ssh.host=your_ssh_host
ssh.port=22
ssh.username=your_ssh_username
ssh.password=your_ssh_password
ssh.remote.host=your_remote_host
ssh.remote.port=3306
ssh.local.port=3307 